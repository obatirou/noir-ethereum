    "@ethereumjs/rlp": "^5.0.2",
    "@ethereumjs/tx": "^5.4.0",
    "@noir-lang/noir_codegen": "^1.0.0-beta.3",
    "@types/node": "^22",
    "dotenv": "^16.4.7",
    "klarity": "^0.0.3",
    "lefthook": "^1.11.8",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  },
  "peerDependencies": {
    "viem": "^2.26.4"
  },
  "dependencies": {
    "@ethereumjs/trie": "^6.2.1",
