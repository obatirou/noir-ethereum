    "ethereum",
    "rlp",
    "merkle-patricia-tree",
    "merkle-patricia-proofs",
    "merkle-proof",
    "zero-knowledge",
    "aztec"
  ],
  "license": "MIT",
  "author": {
    "name": "Vedant Chainani",
    "email": "vedant@zkpersona.xyz",
    "url": "https://zkpersona.xyz"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist/**",
